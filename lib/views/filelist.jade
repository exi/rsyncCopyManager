.container
  h2 Filelist

  #filelistModal.modal.hide(tabindex='-1', role='dialog', aria-hidden='true')

  form.form-search.filelist-form-search
    input.input-medium.search-query.filelist-search(type='text', placeholder='Search')
    select.input-medium.filelist-search-range
      option(value='all') All
      option(value='own') Only mine
      option(value='other') Not mine

  .filelist-tree.jqueryFileTree(style='min-height: 100px;')
